{% extends "./layouts/app.twig" %}
{% block style %}
	<style>
		table {
			border: 1px solid;
		};
	</style>
{% endblock %}
{% block title %}
	Keukens
{% endblock %}
{% block content %}
	{% for cuisine in cuisines %}
		<table>
			<tr>
				<td>
					<a href="cuisine/show?id={{ cuisine.id }}">{{ cuisine.name }}</a>
				</td>
			</tr>
		</table>
	{% endfor %}
{% endblock %}
