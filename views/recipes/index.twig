{% extends "./layouts/app.twig" %}
{% block style %}
	<style>
		table {
			border: 1px solid;
		};
	</style>
{% endblock %}
{% block title %}
	Recepten
{% endblock %}
{% block content %}
	{% for recipe in recipes %}
		<table>
			<tr>
				<td>
					{{recipe.name}}
				</td>
				<td>
					{{recipe.type}}
				</td>
				<td>
					{{recipe.level}}</td>
			</tr>
		</table>
	{% endfor %}
{% endblock %}
